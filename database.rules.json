{
  "rules": {
    "status": {
      "$uid": {
        ".read": true,
        ".write": "$uid === auth.uid"
      }
    },
    "typing": {
      "$chatId": {
        ".read": "auth != null",
        "$uid": {
          ".write": "$uid === auth.uid"
        }
      }
    },
    "test": {
      ".read": true,
      ".write": true
    }
  }
}

